#!/bin/sh
#
# PX4 FMUv3 specific board sensors init
#------------------------------------------------------------------------------

rgbled start -I
rgbled_ncp5623c start -I

board_adc start

# Internal I2C bus
#hmc5883 -T -I -R 4 start

# Internal SPI (auto detect ms5611 or ms5607)
if ! ms5611 -T 5607 -s -b 1 start
then
	ms5611 -s -b 1 start
fi

ms5611 -s -b 4 start
